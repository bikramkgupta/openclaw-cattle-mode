# Telegram (required - put this first)
TELEGRAM_BOT_TOKEN=
# Optional: comma-separated Telegram user IDs. If empty, Telegram uses pairing mode.
TELEGRAM_ALLOWFROM=

# Gradient (required)
GRADIENT_API_KEY=

# Optional model providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Optional override (otherwise: gradient/anthropic-claude-opus-4.6)
AGENT_DEFAULT_MODEL=

# Agent identity
AGENT_ID=agent-dev
AGENT_NAME=OpenClaw Dev Agent

# Gateway auth (required)
# Generate with: openssl rand -hex 32
OPENCLAW_GATEWAY_TOKEN=

# Object store (Spaces in prod; RustFS locally)
SPACES_BUCKET=openclaw-dev
SPACES_REGION=us-east-1
SPACES_ACCESS_KEY_ID=rustfsadmin
SPACES_SECRET_ACCESS_KEY=rustfsadmin
S3_ENDPOINT_URL=http://rustfs:9000

# Local S3 image (optional override)
RUSTFS_IMAGE=rustfs/rustfs:1.0.0-alpha.82
